{
  "channels": [
    {
      "name": "theme_cpu_clock",
      "sources": "all",
      "hardwaretype": "Processor",
      "sensortype": "Clock",
      "fallbackToFirstValue": false,
      "extract": [
        {
          "byLabel": [
            {
              "includes": "cpu"
            }
          ]
        }
      ]
    },
    {
      "name": "theme_gpu_clock",
      "sources": "all",
      "hardwaretype": "Graphics",
      "sensortype": "Clock",
      "fallbackToFirstValue": false,
      "extract": [
        {
          "byLabel": [
            {
              "includes": "gpu"
            }
          ]
        }
      ]
    },
    {
      "name": "theme_vram",
      "sources": "all",
      "hardwaretype": "Graphics",
      "sensortype": "Data",
      "fallbackToFirstValue": false,
      "extract": [
        {
          "byLabel": [
            {
              "includes": "Memory Used"
            },
            {
              "includes": "Memory Usage"
            }
          ]
        }
      ]
    },
    {
      "name": "theme_vram_total",
      "sources": "all",
      "hardwaretype": "Graphics",
      "sensortype": "Data",
      "fallbackToFirstValue": false,
      "extract": [
        {
          "byLabel": [
            {
              "includes": "Memory Total"
            }
          ]
        }
      ]
    },
    {
      "name": "theme_vram_percentage",
      "sources": "all",
      "hardwaretype": "Graphics",
      "sensortype": "Usage",
      "fallbackToFirstValue": false,
      "extract": [
        {
          "byLabel": [
            {
              "includes": "Memory",
              "excludes": "Controller"
            }
          ]
        }
      ]
    }
  ]
}
