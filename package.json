{
  "name": "astern",
  "version": "0.0.1",
  "description": "A fast and modern mobro theme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "run-s clean build:app  && run-p sdk:path assets:copy",
    "build:app": "parcel build src/index.pug --out-dir ./theme/astern",
    "start": "parcel src/index.pug --open --out-dir ./.www",
    "sdk:path": "ts-node --skip-project buildScripts/addSdk.ts",
    "assets:copy": "ts-node --skip-project buildScripts/addMobroAssets.ts",
    "clean": "run-p clean:theme clean:cache clean:devbuild",
    "clean:theme": "del-cli theme",
    "clean:cache": "del-cli .cache",
    "clean:devbuild": "del-cli .www"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.11.5",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^5.1.3",
    "chalk": "^4.1.0",
    "del-cli": "^3.0.1",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.0",
    "jest": "^26.5.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "pug": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "preact": "^10.5.4",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.7",
    "styled-components": "^5.2.0"
  },
  "alias": {
    "react": "react",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "react-dom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
