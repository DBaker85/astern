{
  "name": "astern",
  "version": "1.0.0",
  "description": "A fast and modern mobro theme",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coverage:publish": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "build": "run-s clean build:app && run-p sdk:path && run-s assets:bundle",
    "build:app": "webpack --config=configs/webpack/webpack.app.prod.js",
    "build:docs": "run-p clean:docs clean:buildscripts && run-p compile:docs compile:ssg && run-s replace:docpaths run:ssg",
    "compile:docs": "webpack --config=configs/webpack/webpack.docs.prod.js",
    "compile:ssg": "webpack --config=configs/webpack/webpack.ssg.js",
    "start": "webpack serve --config=configs/webpack/webpack.app.js",
    "start:docs": "webpack serve --config=configs/webpack/webpack.docs.js",
    "sdk:path": "ts-node --skip-project buildScripts/addSdk.ts",
    "assets:bundle": "ts-node --skip-project buildScripts/bundleMobroAssets.ts",
    "fetch:screenshots": "ts-node --skip-project buildScripts/fetchScreenshots.ts",
    "fetch:releasenotes": "ts-node --skip-project buildScripts/fetchReleaseNotes.ts",
    "replace:docpaths": "ts-node --skip-project buildScripts/replaceDocPaths.ts",
    "run:ssg": "node buildScripts/temp/ssg.js",
    "clean": "run-p clean:theme clean:devbuild clean:docs",
    "clean:theme": "del-cli theme",
    "clean:devbuild": "del-cli .www",
    "clean:docs": "del-cli .docs",
    "clean:buildscripts": "del-cli buildScripts/temp",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token n7wizm09peq"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
